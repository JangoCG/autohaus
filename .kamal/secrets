AWS_SECRETS=$(kamal secrets fetch --adapter 1password --account SXKVK34DNBCI3KXZH3IFQY2Q3Y --from keys/AWS_SES SMTP_USERNAME SMTP_PASSWORD)
SECRETS=$(kamal secrets fetch --adapter 1password --account SXKVK34DNBCI3KXZH3IFQY2Q3Y --from keys/GO_AUFTRAG MASTER_KEY)

RAILS_MASTER_KEY=$(kamal secrets extract MASTER_KEY ${SECRETS})
