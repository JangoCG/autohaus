          <div class="aspect-[4/3] rounded-2xl overflow-hidden ring-1 ring-white/10 relative">
            <%= image_tag "about-sports-car-night.png", class: "w-full h-full object-cover", alt: "GoAuftrag Mission" %>
            <div class="absolute inset-0 bg-gradient-to-tr from-sky-500/20 via-transparent to-orange-500/10"></div>
          </div>
        </div>

        <!-- Content -->
